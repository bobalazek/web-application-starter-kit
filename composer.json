{
    "type": "project",
    "name": "bobalazek/web-application-starter-kit",
    "description": "An Web Application starter kit / boilerplate by Borut Balazek - build with Silex",
    "license": "MIT",
    "authors": [
        {
            "name": "Borut Balazek",
            "email": "bobalazek124@gmail.com",
            "homepage": "http://bobalazek.com",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "silex/silex": "~2.1",
        "doctrine/annotations": "~1.4.0",
        "doctrine/cache": "~1.6.2",
        "doctrine/collections": "~1.4.0",
        "doctrine/common": "~2.7.3",
        "doctrine/dbal": "~2.5.13",
        "doctrine/inflector": "~1.1.0",
        "doctrine/instantiator": "~1.0.5",
        "doctrine/orm": "^2.5",
        "doctrine/migrations": "~1.1",
        "saxulum/saxulum-doctrine-orm-manager-registry-provider": "~3.1",
        "swiftmailer/swiftmailer": "5.*",
        "saxulum/config-service-provider": "~2.0",
        "dflydev/doctrine-orm-service-provider": "~2.0",
        "cocur/slugify": "~1.1",
        "pelago/emogrifier": "dev-master",
        "knplabs/knp-components": "~1.2",
        "ua-parser/uap-php": "~3.4",
        "oro/doctrine-extensions": "~1.2",
        "mobiledetect/mobiledetectlib": "^2.8",
        "symfony/translation": "^3.3",
        "symfony/security": "^3.3",
        "symfony/framework-bundle": "^3.3",
        "symfony/validator": "^3.3",
        "symfony/form": "^3.3",
        "symfony/config": "^3.3",
        "symfony/twig-bridge": "^3.3",
        "symfony/security-csrf": "^3.3",
        "symfony/doctrine-bridge": "^3.3",
        "ocramius/proxy-manager": "~2.0.4",
        "vlucas/phpdotenv": "^2.4",
        "silex/web-profiler": "^2.0",
        "symfony/yaml": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "symfony/browser-kit": "^3.3",
        "symfony/phpunit-bridge": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "src/Application/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Application\\Test\\": "tests/Application/Test/"
        }
    },
    "scripts": {
        "post-install-cmd": "Application\\Tool\\Composer::postInstallCmd",
        "post-update-cmd": "Application\\Tool\\Composer::postUpdateCmd"
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": false
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
