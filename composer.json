{
    "type": "project",
    "name": "bobalazek/web-application-starter-kit",
    "description": "An Web Application starter kit / boilerplate by Borut Balazek - build on Silex",
    "version": "0.7.1",
    "license": "MIT",
    "authors": [
        {
            "name": "Borut Balazek",
            "email": "bobalazek124@gmail.de",
            "homepage": "http://bobalazek.com",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "silex/silex": "~1.3",
        "symfony/browser-kit":"~2.7,<3.0",
        "symfony/config":"~2.7,<3.0",
        "symfony/css-selector":"~2.7,<3.0",
        "symfony/doctrine-bridge":"~2.7,<3.0",
        "symfony/dom-crawler":"~2.7,<3.0",
        "symfony/filesystem":"~2.7,<3.0",
        "symfony/finder":"~2.7,<3.0",
        "symfony/form":"~2.7,<3.0",
        "symfony/framework-bundle":"~2.7,<3.0",
        "symfony/locale":"~2.7,<3.0",
        "symfony/monolog-bridge":"~2.7,<3.0",
        "symfony/options-resolver":"~2.7,<3.0",
        "symfony/process":"~2.7,<3.0",
        "symfony/security":"~2.7,<3.0",
        "symfony/security-csrf":"~2.7,<3.0",
        "symfony/serializer":"~2.7,<3.0",
        "symfony/stopwatch":"~2.7,<3.0",
        "symfony/translation":"~2.7,<3.0",
        "symfony/twig-bridge":"~2.7,<3.0",
        "symfony/validator":"~2.7,<3.0",
        "symfony/yaml":"~2.7,<3.0",
        "doctrine/dbal":"~2.5",
        "doctrine/migrations":"1.0.*@dev",
        "doctrine/orm":"~2.5",
        "saxulum/saxulum-doctrine-orm-manager-registry-provider": "~2.1",
        "swiftmailer/swiftmailer": "5.*",
        "igorw/config-service-provider": "~1.2",
        "dflydev/doctrine-orm-service-provider": "1.0.6",
        "cocur/slugify": "~1.1",
        "pelago/emogrifier": "dev-master",
        "knplabs/knp-components": "~1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1"
    },
    "autoload": {
        "psr-0": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Application\\Test\\": "test/Application/Test/"
        }
    },
    "scripts": {
        "post-install-cmd": "Application\\Tool\\Composer::postInstallCmd",
        "post-update-cmd": "Application\\Tool\\Composer::postUpdateCmd"
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
